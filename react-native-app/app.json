{
  "expo": {
    "name": "EcoSync",
    "slug": "ecosync",
    "scheme": "ecosync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/logo_gif.gif",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/brand/logo_with_name.png",
      "resizeMode": "contain",
      "backgroundColor": "#ddebe7",
      "imageBackgroundTransparent": true,
      "title": "EcoSync"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand/logo_gif.gif",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dncc.ecosync"
    },
    "web": {
      "favicon": "./assets/brand/logogif.ico"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-image-picker",
      "expo-location",
      "expo-notifications"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d7822320-c5bf-46a5-8865-05e768bc3285"
      }
    }
  }
}
